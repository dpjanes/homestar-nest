{
  "@context": {
    "@base": "file:///nest-protect",
    "@vocab": "file:///nest-protect#",
    "iot": "https://iotdb.org/pub/iot#",
    "schema": "http://schema.org/",
    "iot-purpose": "https://iotdb.org/pub/iot-purpose#",
    "iot-facet": "https://iotdb.org/pub/iot-facet#",
    "iot:facet": {
      "@id": "https://iotdb.org/pub/iot#facet",
      "@type": "@id"
    },
    "iot:purpose": {
      "@id": "https://iotdb.org/pub/iot#purpose",
      "@type": "@id"
    },
    "iot:type": {
      "@id": "https://iotdb.org/pub/iot#type",
      "@type": "@id"
    }
  },
  "@id": "",
  "@type": "iot:Model",
  "schema:name": "NestProtect",
  "schema:description": "Nest Protect",
  "iot:facet": [
    "iot-facet:security"
  ],
  "iot:attribute": [
    {
      "@type": "iot:Attribute",
      "@id": "#co_alarm_state",
      "schema:name": "carbon-dioxide",
      "iot:purpose": "iot-purpose:sensor.chemical.carbon-dioxide",
      "iot:type": "iot:type.string",
      "iot:read": true
    },
    {
      "@type": "iot:Attribute",
      "@id": "#smoke_alarm_state",
      "schema:name": "smoke",
      "iot:purpose": "iot-purpose:sensor.smoke",
      "iot:type": "iot:type.string",
      "iot:read": true
    },
    {
      "@type": "iot:Attribute",
      "@id": "#battery_health",
      "schema:name": "battery",
      "iot:purpose": "iot-purpose:sensor.battery",
      "iot:type": "iot:type.string",
      "iot:read": true
    }
  ]
}
